local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/Squex01/Rayfield/main/source'))()

if not Library then
   error("Failed to load library.")
end

local Window = Library:CreateWindow({
   Name = "Squex Hub | Muscle Legends",
   LoadingTitle = "Squex Hub | Muscle Legends",
   LoadingSubtitle = "by Squex01",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Squex Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "uJSGzFe7uY",
      RememberJoins = true
   },
   KeySystem = true,
   KeySettings = {
      Title = "Squex Hub",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/uJSGzFe7uY)",
      FileName = "SquexKey",
      SaveKey = true,
      GrabKeyFromSite = true,
      Key = "https://raw.githubusercontent.com/Squex01/Key/main/key"
   }
})

local Tab = Window:CreateTab("Main", 4483362458)
local Section1 = Tab:CreateSection("AutoFarm")
local Paragraph1 = Tab:CreateParagraph({Title = "Warning", Content = "Autofarm and AntiAFK is buggy, do not press these buttons if you experience bugs."})

local Slider1 = Tab:CreateSlider({
   Name = "WalkSpeed",
   Range = {16, 250},
   Increment = 10,
   Suffix = "WalkSpeed",
   CurrentValue = 16,
   Flag = "Slider1",
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

local Slider2 = Tab:CreateSlider({
   Name = "JumpPower",
   Range = {50, 300},
   Increment = 10,
   Suffix = "JumpPower",
   CurrentValue = 50,
   Flag = "Slider2",
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
   end,
})

local Toggle1 = Tab:CreateToggle({
   Name = "Auto Lift",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
      _G.AutoLift = Value
      while _G.AutoLift do
         wait(0.1)
         local A_1 = "GainMuscle"
         local Event = game:GetService("ReplicatedStorage").RemoteEvent
         Event:FireServer(A_1)
      end
   end,
})

local Toggle2 = Tab:CreateToggle({
   Name = "Auto Rebirth",
   CurrentValue = false,
   Flag = "Toggle2",
   Callback = function(Value)
      _G.AutoRebirth = Value
      while _G.AutoRebirth do
         wait(0.1)
         local A_1 = "RebirthRequest"
         local Event = game:GetService("ReplicatedStorage").RemoteEvent
         Event:FireServer(A_1)
      end
   end,
})

local Toggle3 = Tab:CreateToggle({
   Name = "Auto Sell",
   CurrentValue = false,
   Flag = "Toggle3",
   Callback = function(Value)
      _G.AutoSell = Value
      while _G.AutoSell do
         wait(0.1)
         local A_1 = "SellRequest"
         local Event = game:GetService("ReplicatedStorage").RemoteEvent
         Event:FireServer(A_1)
      end
   end,
})

local Toggle4 = Tab:CreateToggle({
   Name = "AutoFarm",
   CurrentValue = false,
   Flag = "Toggle4",
   Callback = function(Value)
      _G.AutoFarm = Value
      while _G.AutoFarm do
         wait(0.1)
         local A_1 = "GainMuscle"
         local Event = game:GetService("ReplicatedStorage").RemoteEvent
         Event:FireServer(A_1)
      end
   end,
})

local Section2 = Tab:CreateSection("AntiAFK")
local Paragraph2 = Tab:CreateParagraph({Title = "Warning", Content = "Do not use AntiAFK, it's very buggy and can kick you out of the game."})

local Toggle5 = Tab:CreateToggle({
   Name = "Anti AFK",
   CurrentValue = false,
   Flag = "Toggle5",
   Callback = function(Value)
      _G.AntiAFK = Value
      while _G.AntiAFK do
         wait(0.1)
         local vu = game:GetService("VirtualUser")
         game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
         end)
      end
   end,
})
